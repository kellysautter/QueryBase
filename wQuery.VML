/*************************************************************************************************
**    
**    OPERATION: SAVE_Document
**    
*************************************************************************************************/
DIALOG OPERATION
SAVE_Document( VIEW ViewToWindow )

   VIEW zqMDocO2   BASED ON LOD  zqMDocO
   VIEW zqMDocOLST BASED ON LOD  zqMDocO
   VIEW mQryRpt    BASED ON LOD  mQryRpt
   VIEW zqFrame    REGISTERED AS zqFrame
   VIEW zqSQuery   REGISTERED AS zqSQuery
   VIEW zqMDocO    REGISTERED AS zqMDocO
   VIEW QueryView
   VIEW KZXMLPGO
   STRING ( 4 ) Suffix
   STRING ( 256 ) OutputFile
   
   // Make sure that a TemplateFileName is specified.
   IF zqMDocO.MergeDocument.TemplateFileName = ""
      MessageSend( ViewToWindow, "DirNameReq", "", "File Directory/Name is required.", zMSGQ_OBJECT_CONSTRAINT_ERROR, 0 )
      RETURN 2
   END
   
   // A Document Object Name must be specified.
   IF zqMDocO.MergeDocument.ObjectName = ""
      MessageSend( ViewToWindow, "ObjectReq", "", "Object is required.", zMSGQ_OBJECT_CONSTRAINT_ERROR, 0 )
      RETURN 2
   END
   
   // Make sure that a Document by the same name doesn't exist.
   ACTIVATE zqMDocO2 WHERE zqMDocO2.MergeDocument.Name  = zqMDocO.MergeDocument.Name 
                       AND zqMDocO2.MergeDocument.ID   != zqMDocO.MergeDocument.ID 
   IF RESULT >= 0  
      MessageSend( ViewToWindow, "", "", "A Document by the same Name currently exists", zMSGQ_OBJECT_CONSTRAINT_ERROR, 0 )
      RETURN 2
   END
   
   ReturnSuffixOfFileName( Suffix, zqMDocO.MergeDocument.TemplateFileName )
   IF Suffix = ""
      MessageSend( ViewToWindow, "", "TemplateSuffix", "Your template file name must have a file suffix type (.docx,.odt etc).", zMSGQ_OBJECT_CONSTRAINT_ERROR, 0 )
      RETURN 2
   END
   
   IF zqMDocO.MergeDocument.OutputFileName = ""
      zqMDocO.MergeDocument.OutputFileName = zqMDocO.MergeDocument.TemplateFileName
   END
   ReturnSuffixOfFileName( Suffix, zqMDocO.MergeDocument.OutputFileName )
   IF Suffix = ""
      MessageSend( ViewToWindow, "", "TemplateSuffix", "Your output file name must have a file suffix type (.docx,.odt etc).", zMSGQ_OBJECT_CONSTRAINT_ERROR, 0 )
      RETURN 2
   END
   
   // If the document type is Email, make sure the suffix of all files is
   // "txt" or HTML.  Otherwise, make sure the suffix of all files is RTF.
   /* 11/27/2024 DonC - Don't know why this isn't filled in, so have just commented it out.
   IF zqMDocO.MergeDocument.DocType = "T"
      ReturnSuffixOfFileName( Suffix, zqMDocO.MergeDocument.TemplateFileName )
      IF Suffix != "txt"
         
         //RETURN 2
      END
   END */

   // Commit document.
   COMMIT zqMDocO
   IF RESULT < 0
      MessageSend( ViewToWindow, "", "", "DB Error - Contact Systems Support", zMSGQ_OBJECT_CONSTRAINT_ERROR, 0 )
      RETURN 2
   END
   
   // Take action based on previous page.
   GET VIEW KZXMLPGO NAMED "KZXMLPGO"
   IF KZXMLPGO.PagePath.LastPageName = "wQueryDocumentMaintList"  
      // Since we came from Document List, add/update the entry in the list.
      GET VIEW zqMDocOLST NAMED "zqMDocOLST"
      IF RESULT >= 0 
         IF zqMDocOLST.MergeDocument EXISTS
            IF zqMDocOLST.MergeDocument.ID = zqMDocO.MergeDocument.ID
               EXCLUDE zqMDocOLST.MergeDocument
            END
         END
         INCLUDE zqMDocOLST.MergeDocument FROM  zqMDocO.MergeDocument
      END
   ELSE
      // Since we came from Report (Packaged Query), relink the Document into the report.
      GET VIEW mQryRpt NAMED "mQryRpt"
      IF RESULT >= 0
         IF mQryRpt.MergeDocument DOES NOT EXIST
            INCLUDE mQryRpt.MergeDocument FROM zqMDocO.MergeDocument 
         ELSE
            IF zqMDocO.MergeDocument.ID = mQryRpt.MergeDocument.ID 
               RelinkInstanceToInstance( mQryRpt, "MergeDocument", zqMDocO, "MergeDocument" )
            END
         END
      END
   END
   
   DropObjectInstance( zqMDocO )

  // Simply drop the new Frame.
   DropObjectInstance( zqFrame )
   DropObjectInstance( zqSQuery )
   GET VIEW QueryView NAMED "QueryView"
   IF RESULT >= 0
      DropObjectInstance( QueryView )
   END
   
END

