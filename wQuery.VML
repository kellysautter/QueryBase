/*************************************************************************************************
**    
**    OPERATION: GOTO_PackagedQueries
**    
*************************************************************************************************/
DIALOG OPERATION
GOTO_PackagedQueries( VIEW ViewToWindow )

   VIEW mQryRptLST  BASED ON LOD  mQryRpt
   VIEW zqSCatagLST BASED ON LOD  zqSCatag
   VIEW mUGroupsLST BASED ON LOD  mUGroups
   VIEW sAppMgr     REGISTERED AS sAppMgr
   VIEW mUser       REGISTERED AS mCurrentUser
   INTEGER nRC
   STRING ( 6 )  szPermissions 
   
   // Going to Packaged Queryies from this menu item allows existing Query Reports to be maintained and new ones to be
   // created from whatever Queries that the User is authorized to maintain. Authorization is done by User Group.
   
   // Administrator functionality is required for this function.
   IF mUser.User.UserName != "Admin" AND mUser.User.QueryAdminAuthority != "Y"
      MessageSend ( ViewToWindow, "AccDenied", "Access",
                    "Administrator authority is required for this function.",
                    zMSGQ_OBJECT_CONSTRAINT_ERROR, 0 )
      SetWindowActionBehavior( ViewToWindow, zWAB_StayOnWindow, "", "" )
      RETURN 2
   END
   
   IF sAppMgr.SplitWindow DOES not EXIST 
      CREATE ENTITY sAppMgr.SplitWindow 
   END 

   sAppMgr.SplitWindow.WindowName = sAppMgr.FunctionalArea.dFunctionalAreaTr +  " > " + sAppMgr.FunctionalSubArea.dFunctionalSubAreaTr 

   // Activate the list of Query Reports based on User authority.
   // Also, if the User has Admin Authority, take them to the Maintenance and Run window.
   // Otherwise, take them to only to the Run window, or if they have NO authority, return an error message.
TraceLineS( "*** Before Activate Reports", "" )
   nRC = ActivateUserReportList( mQryRptLST, mUser )
   IF nRC >= 0
      NAME VIEW mQryRptLST "mQryRptLST"
   END
   IF mUser.User.UserName = "Admin" OR mUser.User.QueryAdminAuthority = "Y"
      // Go to Maintenance and Run page.
      SetWindowActionBehavior( ViewToWindow, zWAB_ReplaceWindowWithModalWindow, "wQueryR", "QueryReportMaintList" )
   ELSE
      // Go to Run only page.
      SetWindowActionBehavior( ViewToWindow, zWAB_ReplaceWindowWithModalWindow, "wQueryR", "" )
   END

   // Make sure list of Stored Query Catagories exists.
   GET VIEW zqSCatagLST NAMED "zqSCatagLST"
   IF RESULT < 0
       ACTIVATE zqSCatagLST RootOnlyMultiple
       NAME VIEW zqSCatagLST "zqSCatagLST"    
   END
   
   // Always activate a new User Group List to pick up changes someone else may have made.
   GET VIEW mUGroupsLST NAMED "mUGroupsLST"
   IF RESULT >= 0
      DropObjectInstance( mUGroupsLST )
   END
   ACTIVATE mUGroupsLST Multiple
         //RESTRICTING mUGroupsLST.SecuritySet TO mUGroupsLST.SecuritySet.ID = 0
   NAME VIEW mUGroupsLST "mUGroupsLST" 
   OrderEntityForView( mUGroupsLST, "UserGroup", "GroupName A" )
   SET CURSOR FIRST mUGroupsLST.UserGroup  
   
END

