/*************************************************************************************************
**    
**    OPERATION: CHECK_ToPDF
**    
*************************************************************************************************/
DIALOG OPERATION
CHECK_ToPDF( VIEW ViewToWindow )

   VIEW zqMDocOLST REGISTERED AS zqMDocOLST

   IF zqMDocOLST.MergeDocument.DocType = "R"
      zqMDocOLST.MergeDocument.wToPDF = ""
      MessageSend( ViewToWindow, "wQueryNoPDF", "", "An RTF document cannot currently be converted to PDF.", zMSGQ_OBJECT_CONSTRAINT_ERROR, 0 )
      RETURN 2
   END
   
   IF zqMDocOLST.MergeDocument.DocType = "T" OR 
      zqMDocOLST.MergeDocument.DocType = "H" OR 
      
      zqMDocOLST.MergeDocument.wToPDF = ""
      MessageSend( ViewToWindow, "wQueryNoPDF", "", "This document type cannot be converted to PDF.", zMSGQ_OBJECT_CONSTRAINT_ERROR, 0 )
      RETURN 2
   END
   
END

