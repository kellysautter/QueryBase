   END

   // Commit document.
   COMMIT zqSCatag
   IF RESULT < 0
      MessageSend( ViewToWindow, "", "", "DB Error - Contact Systems Support", zMSGQ_OBJECT_CONSTRAINT_ERROR, 0 )
      RETURN 2
   END
   
   // Reactivate the list.
   DropObjectInstance( zqSCatagLST )

