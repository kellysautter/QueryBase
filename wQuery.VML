*************************************************************************************************/
DIALOG OPERATION
InitializeHostVariables( VIEW ViewToWindow )

   VIEW sHost REGISTERED AS sHost
   STRING ( 50 )  szLPLRName
   
   // Initialize null Host variables for Query.
   
   // Make sure LPLR Name is entered.
   //IF sHost.Host.wLPLR_TempName = ""
   IF sHost.Host.wLPLR_TempName = ""
      MessageSend( ViewToWindow, "", "Host Initialize",
                   "An LPLR Name must be specified.",
                   zMSGQ_OBJECT_CONSTRAINT_ERROR, 0 )
      RETURN 2
   END
   
   szLPLRName         = sHost.Host.wLPLR_TempName
   sHost.Host.DateFormat                       = "MM/DD/YYYY"
   sHost.Host.TimeFormat                       = "HH:MM AM"
   sHost.Host.TempDir                          = "c:/Temp/"
   sHost.Host.QueryLODsSourceDirectory         = "C:/Program Files (x86)/apache-tomcat-9.0.31/webapps/" + szLPLRName + "/query/"
   sHost.Host.MergeDocumentsEmailSourceAddress = "MergeEmail@comcast.net"
   sHost.Host.MergeDocumentTemplatesDirectory  = "C:/" + szLPLRName + "/MergeDocTemplates/"
   sHost.Host.ODT_DOCX_FileUnzipZipDirectory   = "C:/" + szLPLRName + "/MergeDocsZipUnzip/"
   sHost.Host.MergeDocumentOutputsDirectory    = "C:/" + szLPLRName + "/MergeDocOutput/"
   sHost.Host.SecureEmailWorkDirectory         = "C:/" + szLPLRName + "/SecureEmailWork/"
   sHost.Host.EmailMaximumGroupSend            = 100

END

