/*************************************************************************************************
**    
**    OPERATION: DOWNLOAD_MergeDocuments
**    
*************************************************************************************************/
DIALOG OPERATION
DOWNLOAD_MergeDocuments( VIEW ViewToWindow )

   VIEW wXferO     REGISTERED AS wXferO
   VIEW sHost      REGISTERED AS sHost
   VIEW mUser      REGISTERED AS mCurrentUser
   VIEW sAppMgr    REGISTERED AS sAppMgr
   VIEW zqMDocOLST REGISTERED AS zqMDocOLST
   VIEW mConList   BASED ON LOD  mConList
   VIEW QueryView 
   VIEW vQualObject
   VIEW ResultSet 
   STRING ( 50 )  szObjectName
   STRING ( 50 )  szRootEntityName
   STRING ( 50 )  szLoopingEntityName
   STRING ( 50 )  szFunctionalAreaName
   STRING ( 50 )  szDataEntityName
   STRING ( 50 )  szConListName
   STRING ( 50 )  szSelectFieldName
   STRING ( 500 ) szTemplateDirectoryFileName
   STRING ( 500 ) szOutputDirectoryFileName
   STRING ( 500 ) szMsg
   STRING ( 1 )   szSelected
   INTEGER nRC
   
   // Process an ODT Document for Print.
   
   // Make sure Template and Output file components are specified.
   IF sHost.Host.MergeDocumentTemplatesDirectory = "" 
      MessageSend( ViewToWindow, "", "", "The Host Templates Directoryvalue has not been specified.", zMSGQ_OBJECT_CONSTRAINT_ERROR, 0 )
      RETURN 2
   END
   IF zqMDocOLST.MergeDocument.TemplateFileName = "" 
      MessageSend( ViewToWindow, "", "", "The Document Definition does not include a Template File name.", zMSGQ_OBJECT_CONSTRAINT_ERROR, 0 )
      RETURN 2
   END
   IF sHost.Host.MergeDocumentOutputsDirectory = ""
      MessageSend( ViewToWindow, "", "", "A Host Outputs Directory value has not been specified.", zMSGQ_OBJECT_CONSTRAINT_ERROR, 0 )
      RETURN 2
   END
   IF zqMDocOLST.MergeDocument.OutputFileName = "" 
      MessageSend( ViewToWindow, "", "", "The Document Definition does not include an Output File name.", zMSGQ_OBJECT_CONSTRAINT_ERROR, 0 )
      RETURN 2
   END
   szTemplateDirectoryFileName = sHost.Host.MergeDocumentTemplatesDirectory + zqMDocOLST.MergeDocument.TemplateFileName
   szOutputDirectoryFileName   = sHost.Host.MergeDocumentOutputsDirectory + zqMDocOLST.MergeDocument.OutputFileName
   
   // Make sure Document Type is set.
   IF zqMDocOLST.MergeDocument.DocType = ""
      MessageSend( ViewToWindow, "", "", "The Document Type must be specified.", zMSGQ_OBJECT_CONSTRAINT_ERROR, 0 )
      RETURN 2
   END
   
   // Delete any existing ActivateID_List entries in preparation for activates below.
   FOR EACH wXferO.ActivateID_List 
      DELETE ENTITY wXferO.ActivateID_List NONE 
   END
   
   // Activate LOD to get RootEntityName.
   szObjectName = zqMDocOLST.MergeDocument.ObjectName 
   nRC = ActivateQueryViewByName( zqMDocOLST, QueryView, szObjectName )
   IF nRC < 0
      RETURN 2
   END
   NAME VIEW QueryView "QueryView"
   szRootEntityName = QueryView.LOD_EntityParent.Name
   
   // Note that the wXferO.QueryObject entries have been set up for the User's User Group authority.
   SET CURSOR FIRST wXferO.QueryObject WHERE wXferO.QueryObject.ObjectName = szObjectName   
   IF RESULT < zCURSOR_SET
      MessageSend( ViewToWindow, "", "", "The User does not have the authority to process this Document", zMSGQ_OBJECT_CONSTRAINT_ERROR, 0 )
      RETURN 2
   END
   
   // Set up work areas for object.
   szLoopingEntityName = wXferO.QueryObject.ContactListCreateEntityName
   szDataEntityName    = wXferO.QueryObject.ContactListIncludeEntityName
   szConListName       = "mConList" + wXferO.QueryObject.ContactListType 
   IF wXferO.QueryObject.ContactListType = "P"       // Check here is only for Person
      szSelectFieldName   = "wSelectedFlag"
   ELSE
      szSelectFieldName   = "wSelected"
   END
   
   // The following code was removed by DonC on 11/27/2024 because authority is controlled by User Groups.
   /*szFunctionalAreaName = sAppMgr.FunctionalSubArea.FunctionalSubAreaName 
   nRC = ZeidonStringFind( szFunctionalAreaName, 1, szLoopingEntityName)
   IF nRC < 0 AND ( szFunctionalAreaName = "Students" AND szLoopingEntityName != "ContactListItem" )
      szMsg = "This query with a root of '" + szLoopingEntityName + "' is not for functional area '" + szFunctionalAreaName + "."
      MessageSend( ViewToWindow, "", "", szMsg, zMSGQ_OBJECT_CONSTRAINT_ERROR, 0 )
      RETURN 2
   END*/
   
   // Get view to selected Contact List.
   
   GET VIEW mConList NAMED szConListName
   IF RESULT < 0
      szMsg = "Error finding a contact list for '" + wXferO.QueryObject.ExternalName + "'."
      MessageSend( zqMDocOLST, "", "", szMsg, zMSGQ_OBJECT_CONSTRAINT_ERROR, 0 )
      RETURN 2
   END
   
   nRC = SetCursorFirstEntityByString( mConList, szLoopingEntityName, szSelectFieldName, "Y", "" )
   IF nRC < 0
      szMsg = "No Contact List entry was selected for '" + wXferO.QueryObject.ExternalName + "' contact list."
      MessageSend( zqMDocOLST, "", "", szMsg, zMSGQ_OBJECT_CONSTRAINT_ERROR, 0 )
      RETURN 2
   END
      
   // Build ActivateID_List entries for each selected Contact List entry.
   nRC = SetCursorFirstEntity( mConList, szLoopingEntityName, "" )
   LOOP WHILE nRC >= zCURSOR_SET 
      GetStringFromAttribute( szSelected, mConList, szLoopingEntityName, szSelectFieldName )
      IF szSelected = "Y" 
         // Add ID to activate list.
         CREATE ENTITY wXferO.ActivateID_List
         SetAttributeFromAttribute( wXferO, "ActivateID_List", "ID", mConList, szDataEntityName, "ID" )
      END
      nRC = SetCursorNextEntity( mConList, szLoopingEntityName, "" ) 
   END
   
   // Format output file of merged entries.
   GenerateQualFromEntityList( vQualObject, wXferO, "ActivateID_List", "", 0 )  
   NAME VIEW vQualObject "vQualObjectMergePrint"
   SET CURSOR FIRST vQualObject.KeyList 
   IF RESULT >= zCURSOR_SET
       vQualObject.QualAttrib.Oper = "IN"
   END
   ActivateObjectInstance( ResultSet, szObjectName, ViewToWindow, vQualObject, zMULTIPLE )
   NAME VIEW ResultSet "ResultSetPrint"
   NAME VIEW ResultSet "ResultSet" // DOCX/ODT uses ResultSet.
   
   
   // Output file and insert operation depend on Document Type.
   IF zqMDocOLST.MergeDocument.DocType = "O"
      // Document Type is ODT.
      IF szOutputDirectoryFileName = ""
         szOutputDirectoryFileName   = sHost.Host.TempDir + mUser.User.UserName + "QueryResult.odt"
      ELSE
         // Check that the file has .odt at the end.
         IF ZeidonStringFind( szOutputDirectoryFileName, 1, ".odt") < 0
            szOutputDirectoryFileName = szOutputDirectoryFileName + ".odt"
         END
      END
      wXferO.QueryValues.OutputFileName = szOutputDirectoryFileName
      
      // Merge the data into the template.
      nRC = InsertOI_DataIntoODT_Template( ResultSet, 
                                           szOutputDirectoryFileName,
                                           szTemplateDirectoryFileName, 
                                           szRootEntityName )
      IF nRC = 0 AND zqMDocOLST.MergeDocument.wToPDF = "Y"
         IF ConvertFileToPDF( szOutputDirectoryFileName, "ODT" ) = 0
            zSearchAndReplace( szOutputDirectoryFileName, 500, ".odt", ".pdf" ) 
            MessageSend( ViewToWindow, "", "",
                         "This document was unable to be printed to a pdf.",
                         zMSGQ_OBJECT_CONSTRAINT_ERROR, 0 )
         END                                          
         wXferO.QueryValues.OutputFileName = szOutputDirectoryFileName
      END
   ELSE
   IF zqMDocOLST.MergeDocument.DocType = "D"
      // Document Type is DOCX.
      IF szOutputDirectoryFileName = ""
         szOutputDirectoryFileName   = sHost.Host.TempDir + mUser.User.UserName + "QueryResult.docx" 
      ELSE
         // Check that the file has .odt at the end.
         IF ZeidonStringFind( szOutputDirectoryFileName, 1, ".docx") < 0
            szOutputDirectoryFileName = szOutputDirectoryFileName + ".docx"
         END
      END
      wXferO.QueryValues.OutputFileName = szOutputDirectoryFileName
      
      // Merge the data into the template.
      nRC = InsertOI_DataIntoDOCX_Template( ResultSet, 
                                            szOutputDirectoryFileName,
                                            szTemplateDirectoryFileName, 
                                            szRootEntityName )
      IF nRC = 0 AND zqMDocOLST.MergeDocument.wToPDF = "Y"
         IF ConvertFileToPDF( szOutputDirectoryFileName, "DOCX" ) = 0
            zSearchAndReplace( szOutputDirectoryFileName, 500, ".docx", ".pdf" )
            MessageSend( ViewToWindow, "", "",
                         "This document was unable to be printed to a pdf.",
                         zMSGQ_OBJECT_CONSTRAINT_ERROR, 0 )
         END                                           
         wXferO.QueryValues.OutputFileName = szOutputDirectoryFileName
      END
   ELSE
      // Document Type is processed as text, such as HTML or RTF.
      IF szOutputDirectoryFileName = ""
         szOutputDirectoryFileName   = sHost.Host.TempDir + mUser.User.UserName + "QueryResult.html" 
      END
      wXferO.QueryValues.OutputFileName = szOutputDirectoryFileName
      
      // Merge the data into the template.
      nRC = InsertOI_DataIntoTemplateFile( ResultSet, 
                                           szOutputDirectoryFileName,
                                           szTemplateDirectoryFileName, 
                                           szRootEntityName )
   END
   END
   IF nRC = 2
      wXferO.QueryValues.OutputFileName = ""
      RETURN 2
   END
   IF nRC < 0
      wXferO.QueryValues.OutputFileName = ""
      MessageSend( ViewToWindow, "", "",
                   "An error was encountered during Merge processing.",
                   zMSGQ_OBJECT_CONSTRAINT_ERROR, 0 )
      RETURN 2
   END

   // Set up File Name for saving to local directory.
   IF wXferO.QueryValues DOES NOT EXIST
      CREATE ENTITY wXferO.QueryValues 
   END
   wXferO.QueryValues.OutputFileName = szOutputDirectoryFileName

END

