/*************************************************************************************************
**    
**    OPERATION: SAVE_QueryObject
**    
*************************************************************************************************/
DIALOG OPERATION
SAVE_QueryObject( VIEW ViewToWindow )

   VIEW mQryObj2   BASED ON LOD  mQryObj
   VIEW mQryObj    REGISTERED AS mQryObj
   VIEW mQryObjLST REGISTERED AS mQryObjLST
   
   // Make sure that Category Name is entered and that it is unique.
   IF mQryObj.QueryObject.ObjectName = ""
      MessageSend( ViewToWindow, "", "", "Object Name is required.", zMSGQ_OBJECT_CONSTRAINT_ERROR, 0 )
      RETURN 2
   END
   
   // If this is a new instance, make sure the Name is unique.
   IF mQryObj.QueryObject.ID  = ""
      ACTIVATE mQryObj2 WHERE mQryObj2.QueryObject.ObjectName = mQryObj.QueryObject.ObjectName 
      IF RESULT >= 0 
         MessageSend( ViewToWindow, "", "", "A Query Object by that Name currently exists.", zMSGQ_OBJECT_CONSTRAINT_ERROR, 0 )
         RETURN 2
      END
   END

   // Commit Query Object.
   COMMIT mQryObj
   IF RESULT < 0
      MessageSend( ViewToWindow, "", "", "DB Error - Contact Systems Support", zMSGQ_OBJECT_CONSTRAINT_ERROR, 0 )
      RETURN 2
   END
   
   // Reactivate the list.
   DropObjectInstance( mQryObjLST )

END

/*************************************************************************************************
**    
**    OPERATION: INCLUDE_UserGroupToQueryObject
**    
*************************************************************************************************/
DIALOG OPERATION
INCLUDE_UserGroupToQueryObject( VIEW ViewToWindow )

   VIEW mQryObj     REGISTERED AS mQryObj
   VIEW mUGroupsLST REGISTERED AS mUGroupsLST

   // Include the select Group into the Query Object if it doesn't already exist.
   SET CURSOR FIRST mQryObj.UserGroup WHERE mQryObj.UserGroup.GroupName = mUGroupsLST.UserGroup.GroupName 
   IF RESULT < zCURSOR_SET
      INCLUDE mQryObj.UserGroup FROM mUGroupsLST.UserGroup 
   END
   
   // Reorder the entries.
   OrderEntityForView( mQryObj, "UserGroup", "GroupName A" )  
   SET CURSOR FIRST mQryObj.UserGroup 

END

