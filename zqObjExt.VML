      GetStringFromAttributeByContext( szDateMax, zqObjExt, "SummarySelectionCriteria", "RangeMaximumDate", "", 20 )
      IF szDateMin = "" AND szDateMax = ""
         szValue = "No Date Range values specified."
      ELSE 
         IF szDateMin != "" AND szDateMax != ""
            // Both min and max are specified.
            szValue = "Min: " + szDateMin + " / Max: " + szDateMax
         ELSE
             IF szDateMin = "" 
               // Max only is specified.
               szValue = "Max: " + szDateMax
            ELSE
               // Min only is specified.
               szValue = "Min: " + szDateMin 
            END
         END
      END
   ELSE
   IF szType = "Range"  
      GetStringFromAttributeByContext( szDecimalMin, zqObjExt, "SummarySelectionCriteria", "RangeMinimum", "", 20 )
      GetStringFromAttributeByContext( szDecimalMax, zqObjExt, "SummarySelectionCriteria", "RangeMaximum", "", 20 )
      IF szDecimalMin = "" AND szDecimalMax = ""
         szValue = "No Numeric Range values specified."
      ELSE 
         IF szDecimalMin != "" AND szDecimalMax != ""
            // Both min and max are specified.
            szValue = "Min: " + szDecimalMin + " / Max: " + szDecimalMax
         ELSE
             IF szDecimalMin = "" 
               // Max only is specified.
               szValue = "Max: " + szDecimalMax
            ELSE
               // Min only is specified.
               szValue = "Min: " + szDecimalMin 
            END
         END
      END
   END
   END
   END
   
   StoreStringInRecord( zqObjExt, InternalEntityStructure, InternalAttribStructure, szValue )

        /* end zDERIVED_GET */
   OF   zDERIVED_SET:

        /* end zDERIVED_SET */
   END  /* case */

END
