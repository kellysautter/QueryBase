/*************************************************************************************************
**    
**    OPERATION: dDisplaySelCriteria
**    
*************************************************************************************************/
DERIVED ATTRIBUTE OPERATION
dDisplaySelCriteria( VIEW zqObjExt BASED ON LOD zqObjExt,
                     STRING ( 32 ) InternalEntityStructure,
                     STRING ( 32 ) InternalAttribStructure,
                     SHORT GetOrSetFlag )

   STRING ( 500 ) szValue
   STRING ( 20 )  szType
   STRING ( 20 )  szDateMin
   STRING ( 20 )  szDateMax
   STRING ( 20 )  szDecimalMin
   STRING ( 20 )  szDecimalMax
   INTEGER        Count

   CASE GetOrSetFlag
   OF   zDERIVED_GET:
   
   // Convert the selection criteria into a displayable string.
   
   szType = zqObjExt.SummaryDisplayAttribute.SelectionCriteriaType
//TraceLineS( "*** Type: ", szType )
   
   // Discrete Set
   IF szType = "Discrete Set"
      Count = 0
      FOR EACH zqObjExt.SummaryDisplaySubParameter  
         Count = Count + 1
         IF Count = 1
            szValue = zqObjExt.SummaryDisplaySubParameter.ExternalValue 
         ELSE
            szValue = szValue + " / " + zqObjExt.SummaryDisplaySubParameter.ExternalValue 
         END
      END
   ELSE
   IF szType = "Single Value"
      IF zqObjExt.SummaryDisplayAttribute.SingleValueDecimal != ""
         szValue = zqObjExt.SummaryDisplayAttribute.SingleValueDecimal
      ELSE 
      IF zqObjExt.SummaryDisplayAttribute.SingleValueString != ""
         szValue = zqObjExt.SummaryDisplayAttribute.SingleValueString
      END
      END
   ELSE
   IF szType = "Date Range"  
      GetStringFromAttributeByContext( szDateMin, zqObjExt, "SummaryDisplayAttribute", "RangeMinimumDate", "", 20 )
      GetStringFromAttributeByContext( szDateMax, zqObjExt, "SummaryDisplayAttribute", "RangeMaximumDate", "", 20 )
      IF szDateMin = "" AND szDateMax = ""
         szValue = "No Date Range values specified."
      ELSE 
         IF szDateMin != "" AND szDateMax != ""
            // Both min and max are specified.
            szValue = "Min: " + szDateMin + " / Max: " + szDateMax
         ELSE
             IF szDateMin = "" 
               // Max only is specified.
               szValue = "Max: " + szDateMax
            ELSE
               // Min only is specified.
               szValue = "Min: " + szDateMin 
            END
         END
      END
   ELSE
   IF szType = "Numeric Range"  
      GetStringFromAttributeByContext( szDecimalMin, zqObjExt, "SummaryDisplayAttribute", "RangeMinimum", "", 20 )
      GetStringFromAttributeByContext( szDecimalMax, zqObjExt, "SummaryDisplayAttribute", "RangeMaximum", "", 20 )
      IF szDecimalMin = "" AND szDecimalMax = ""
         szValue = "No Numeric Range values specified."
      ELSE 
         IF szDecimalMin != "" AND szDecimalMax != ""
            // Both min and max are specified.
            szValue = "Min: " + szDecimalMin + " / Max: " + szDecimalMax
         ELSE
             IF szDecimalMin = "" 
               // Max only is specified.
               szValue = "Max: " + szDecimalMax
            ELSE
               // Min only is specified.
               szValue = "Min: " + szDecimalMin 
            END
         END
      END
   ELSE
      IF szType != ""
         szValue = "Display Type, " + szType + ", not implemented."
      END
   END
   END
   END
   END
   
   StoreStringInRecord( zqObjExt, InternalEntityStructure, InternalAttribStructure, szValue )

        /* end zDERIVED_GET */
   OF   zDERIVED_SET:

        /* end zDERIVED_SET */
   END  /* case */

END

/*************************************************************************************************
**    
**    OPERATION: dDisplaySelCriteria
**    
*************************************************************************************************/
DERIVED ATTRIBUTE OPERATION
dSelectionCriteria( VIEW zqObjExt BASED ON LOD zqObjExt,
                    STRING ( 32 ) InternalEntityStructure,
                    STRING ( 32 ) InternalAttribStructure,
                    SHORT GetOrSetFlag )

   STRING ( 500 ) szValue
   STRING ( 20 )  szType
   STRING ( 20 )  szDateMin
   STRING ( 20 )  szDateMax
   STRING ( 20 )  szDecimalMin
   STRING ( 20 )  szDecimalMax
   INTEGER        Count

   CASE GetOrSetFlag
   OF   zDERIVED_GET:
   
   // Convert the selection criteria into a displayable string.
   
   szType = zqObjExt.SummarySelectionCriteria.SummaryType 
   szValue = ""    // Null is default
//TraceLineS( "*** Type: ", szType )
TraceLineS( "****** Start dSelection", "" )
   
   // Discrete Set
   IF szType = "Discrete"
TraceLineS( "****** Discrete", "" )
      Count = 0
      FOR EACH zqObjExt.SummaryEntrySubParameter  
         Count = Count + 1
         IF Count = 1
            szValue = zqObjExt.SummaryEntrySubParameter.ExternalValue 
         ELSE
            szValue = szValue + " / " + zqObjExt.SummaryEntrySubParameter.ExternalValue 
         END
      END
